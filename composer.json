{
  "name": "tfboe/fm-lib",
  "description": "A lumen package with base functionality for all foosball manager applications",
  "type": "library",
  "keywords": [
    "foosball",
    "manager"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Benedikt Klocker",
      "email": "blacktemplar@a1.net"
    }
  ],
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/blacktemplar/jwt-auth-guard"
      }
  ],
  "require": {
    "laravel/lumen-framework": "^8.0",
    "laravel-doctrine/orm": "^1.4.2",
    "laravel-doctrine/extensions": "^1.0.12",
    "gedmo/doctrine-extensions": "^2.4.33",
    "tymon/jwt-auth": "^1.0.0",
    "irazasyed/jwt-auth-guard": "dev-master",
    "doctrine/orm": "^2.6",
    "ext-json": "*"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4.1",
    "phpunit/phpcov": "^8.0",
    "phpmd/phpmd": "^2.6",
    "phpunit/phpunit": "^9.0",
    "fzaninotto/faker": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Tfboe\\FmLib\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tfboe\\FmLib\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
